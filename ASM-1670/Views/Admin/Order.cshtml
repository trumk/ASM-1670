@model List<Order>
@{
    ViewData["Title"] = "Order";
}

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>User ID</th>
            <th>Order Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.UserId</td>
                <td>@order.OrderDate</td>
            </tr>
        }
    </tbody>
</table>

